<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
	</settings>
	<typeAliases>
		<!-- 회원관련 -->
		<typeAlias alias="MemberVO"
			type="com.omgm.member.beans.MemberVO" />
		<typeAlias alias="ManagementVO"
			type="com.omgm.admin.memberManagement.beans.ManagementVO" />
		<!-- 이용후기 -->
		<typeAlias alias="ReviewVO"
			type="com.omgm.user.review.beans.ReviewVO" />
		<typeAlias alias="ReviewReplyVO"
			type="com.omgm.user.review.beans.ReviewReplyVO" />
		<!-- 유치원 -->
		<typeAlias alias="KinderGardenVO"
			type="com.omgm.admin.kinderGarden.beans.KinderGardenVO" />
		<typeAlias alias="KinderGardenRowVO"
			type="com.omgm.admin.kinderGarden.beans.KinderGardenRowVO" />
		<!-- 유저 공통 -->
		<typeAlias alias="KinderGardenInfoVO"
			type="com.omgm.user.common.beans.KinderGardenInfoVO" />
		<typeAlias alias="KinderGardenInfoRowVO"
			type="com.omgm.user.common.beans.KinderGardenInfoRowVO" />
		<!-- 몰 -->
		<typeAlias alias="mallToyVO"
			type="com.omgm.admin.mall.beans.MallToyVO" />
		<typeAlias alias="mallOrder"
			type="com.omgm.admin.mall.beans.MallOrderVO" />
		<typeAlias alias="mallFeedVO"
			type="com.omgm.admin.mall.beans.MallFeedVO" />
		<typeAlias alias="usermallToyVO"
			type="com.omgm.user.mall.beans.UserMallToyVO" />
		<typeAlias alias="usermallFeedVO"
			type="com.omgm.user.mall.beans.UserMallFeedVO" />
		<typeAlias alias="Order"
			type="com.omgm.admin.mall.beans.OrderVO" />
		<!-- -->
		<typeAlias alias="faq" type="com.omgm.user.faq.beans.FAQVO" />
		<typeAlias alias="category"
			type="com.omgm.user.faq.beans.CategoryVO" />
		<typeAlias alias="faq_m"
			type="com.omgm.admin.faq.beans.FAQVO_M" />
		<typeAlias alias="category_m"
			type="com.omgm.admin.category.beans.CategoryVO_M" />
		<typeAlias alias="catcarelog"
			type="com.omgm.user.catcarelog.beans.CatCareLogVO" />
		<typeAlias alias="mypet"
			type="com.omgm.user.mypet.beans.MyPetVO" />
		<typeAlias alias="room" type="com.omgm.user.room.beans.RoomVO"/>
		<typeAlias alias="room_m" type="com.omgm.admin.room.beans.RoomVO_M"/>
	</typeAliases>
	<mappers>
		<!-- 회원 -->
		<mapper resource="mappings/member-mapping.xml" />
		<mapper
			resource="mappings/memberManagement/management-mapping.xml" />
		<!-- 이용후기 -->
		<mapper resource="mappings/review-mapping.xml" />
		<!-- 유치원 -->
		<mapper
			resource="mappings/kinderGarden/kinderGarden-mapping.xml" />
		<!-- 유저 공통 -->
		<mapper resource="mappings/common-mapping.xml" />
		<!-- 몰 -->
		<mapper resource="mappings/mall/productList-mapping.xml" />
		<mapper resource="mappings/mall/order-mapping.xml" />
		<mapper resource="mappings/mall/mall-mapping.xml" />
		<!-- -->
		<mapper resource="mappings/faq-mapping.xml" />
		<mapper resource="mappings/category-mapping.xml" />
		<mapper resource="mappings/faq_m-mapping.xml" />
		<mapper resource="mappings/category_m-mapping.xml" />
		<mapper resource="mappings/catcarelog-mapping.xml" />
		<mapper resource="mappings/mypet-mapping.xml" />
		<mapper resource="mappings/room-mapping.xml" />
		<mapper resource="mappings/room-mapping_m.xml" />
	</mappers>
</configuration>
